{
  "cli": {
    "version": ">= 5.1.0",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "image": "latest"
      },
      "ios": {
        "resourceClass": "m-medium",
        "image": "latest"
      }
    },
    "preview": {
      "developmentClient": false,
      "distribution": "store",
      "ios": {
        "resourceClass": "m-medium"
      },
      "env": {
        "ENVIRONMENT": "preview"
      }
    },
    "production": {
      "distribution": "store",
      "ios": {
        "resourceClass": "m-medium"
      },
      "env": {
        "ENVIRONMENT": "production"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "gil.raz.il@gmail.com"
      }
    }
  }
} 